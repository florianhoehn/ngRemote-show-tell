<apex:page showHeader="false"
           sidebar="false"
           standardStylesheets="false"
           language="en"
           applyBodyTag="false"
           applyHtmlTag="false"
           docType="html">
<html>
    <div class="panel panel-success">
        <div class="panel-heading">
            <h3 class="panel-title">Contacts</h3>
        </div>
        <div class="panel-body">
            <ul>
                <li ng-repeat="contact in vm.contacts">
                    <p>{{contact.Name}}</p>
                </li>
            </ul>
        </div>
    </div>
</html>
</apex:page>