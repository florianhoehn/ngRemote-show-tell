<apex:page showHeader="false"
           sidebar="false"
           standardStylesheets="false"
           language="en"
           applyBodyTag="false"
           applyHtmlTag="false"
           docType="html">
<html>
    <p>Contacts:</p>
    <ul>
        <li ng-repeat="contact in vm.contacts">
            <p>{{contact.Name}}</p>
        </li>
    </ul>
</html>
</apex:page>